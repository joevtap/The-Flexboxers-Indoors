{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {"semi": true, "singleQuote": false, "arrowParens": "avoid", "parser": "flow", "endOfLine":"auto"}],
        "space-before-function-paren": "off",
        "react/prop-types": 0,
        "explicitSpread": "ignore"
    }
}
